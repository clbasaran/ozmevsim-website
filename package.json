{
  "name": "ozmevsim-website",
  "version": "0.1.0",
  "description": "Öz Mevsim Isı Sistemleri Mühendislik - Enterprise Web Sitesi",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:wrangler": "wrangler pages dev --local --d1=ozmevsim_d1=ozmevsim-d1 -- npm run dev",
    "build": "next build",
    "build:cf": "npx @cloudflare/next-on-pages",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx @cloudflare/next-on-pages --watch",
    "deploy": "npm run build && npm run build:cf && npx wrangler pages deploy .vercel/output/static --project-name=ozmevsim-website",
    "deploy:production": "npm run build && npm run build:cf && npx wrangler pages deploy .vercel/output/static --project-name=ozmevsim-website --env production",
    "preview": "npx wrangler pages dev .vercel/output/static --port=3000 --d1=ozmevsim_d1=ozmevsim-d1"
  },
